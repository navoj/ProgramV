<pattern>STACK QUESTIONS POP</pattern>
<template>
<condition name="QUESTIONS">
<li value=""></li>
<li><srai>STACK QUESTIONS POP <get name="QUESTIONS"/></srai></li>
</condition>
</template>
</category>
-->

<category>
	<pattern>STACK QUESTIONS POP</pattern>
		<template>
			<srai>STACK QUESTIONS POP <get name="QUESTIONS"/></srai>
		</template>
</category>

</aiml>