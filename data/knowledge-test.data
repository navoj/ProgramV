	my $predicates	= AIML::Knowledge::getPredicates ( $bot_id );

	if ( exists $predicates->{$name} )
	{
		my $default		= $predicates->{$name}->{default};

		return 0		if $default and ( $value eq $default );
	}

	return 1;
}
